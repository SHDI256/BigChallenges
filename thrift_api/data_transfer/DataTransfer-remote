client = DataTransfer.Client(protocol)
transport.open()

if cmd == 'data_transfer_image':
    if len(args) != 1:
        print('data_transfer_image requires 1 args')
        sys.exit(1)
    pp.pprint(client.data_transfer_image(args[0],))

elif cmd == 'data_transfer_int':
    if len(args) != 1:
